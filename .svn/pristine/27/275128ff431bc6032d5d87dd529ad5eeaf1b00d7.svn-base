@charset "UTF-8";
.sns_area h2 {
	padding-top:20px;
}
* {
	margin: 0;
	padding: 0;
	list-style: none;
}

.sns_box {
	position: relative;
	margin-top: 20px;
	display: inline-block;
	width: 870px;
}

.sns_list {
	position: relative;
	list-style: none;
	display: inline-block;
}
.sns_list li {
	display: inline-block;
	float: left;
}
.sns_list li:not(:first-child) {
	margin-left: 60px;
}
a {
	text-decoration: none;
}

.sns_plus {
	position: relative;
	margin-top: 15px;
	padding-top:14px;
	padding-left:15px;
	padding-bottom:10px;
	text-align: left;
	background-color: #444444;
}

.sns_plus a {
	display:inline-block;
}