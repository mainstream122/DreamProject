		url(http://localhost:8080/Board/js/ckeditor/plugins/link/images/anchor.png?t=H8DA)
		no-repeat right center;
	border: 1px dotted #00f;
	background-size: 16px;
	padding-right: 18px;
	cursor: auto;
}

.content .cke_contents_rtl img.cke_anchor {
	background:
		url(http://localhost:8080/Board/js/ckeditor/plugins/link/images/anchor.png?t=H8DA)
		no-repeat right center;
	border: 1px dotted #00f;
	background-size: 16px;
	width: 16px;
	min-height: 15px;
	height: 1.15em;
	vertical-align: text-bottom;
}

.content div.cke_pagebreak {
	background:
		url(http://localhost:8080/Board/js/ckeditor/plugins/pagebreak/images/pagebreak.gif?t=H8DA)
		no-repeat center center !important;
	clear: both !important;
	width: 100% !important;
	border-top: #999 1px dotted !important;
	border-bottom: #999 1px dotted !important;
	padding: 0 !important;
	height: 7px !important;
	cursor: default !important;
}

