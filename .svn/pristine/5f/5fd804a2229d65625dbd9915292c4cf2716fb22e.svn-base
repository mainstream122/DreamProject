@charset "UTF-8";

.content .cke_editable {
	cursor: text
}

.content .cke_editable img,
.content .cke_editable input, 
.content .cke_editable textarea {
	cursor: default
}

.content img.cke_flash {
	background-image:
		url(http://localhost:8080/Board/js/ckeditor/plugins/flash/images/placeholder.png?t=H8DA);
	background-position: center center;
	background-repeat: no-repeat;
	border: 1px solid #a9a9a9;
	width: 80px;
	height: 80px;
}

.content .cke_editable form {
	border: 1px dotted #FF0000;
	padding: 2px;
}

.content img.cke_hidden {
	background-image:
		url(http://localhost:8080/Board/js/ckeditor/plugins/forms/images/hiddenfield.gif?t=H8DA);
	background-position: center center;
	background-repeat: no-repeat;
	border: 1px solid #a9a9a9;
	width: 16px !important;
	height: 16px !important;
}

.content img.cke_iframe {
	background-image:
		url(http://localhost:8080/Board/js/ckeditor/plugins/iframe/images/placeholder.png?t=H8DA);
	background-position: center center;
	background-repeat: no-repeat;
	border: 1px solid #a9a9a9;
	width: 80px;
	height: 80px;
}

.content .cke_contents_ltr a.cke_anchor, 
.content .cke_contents_ltr a.cke_anchor_empty,
.content .cke_editable.cke_contents_ltr a[name], 
.content .cke_editable.cke_contents_ltr a[data-cke-saved-name]
	{
	background:
		url(http://localhost:8080/Board/js/ckeditor/plugins/link/images/anchor.png?t=H8DA)
		no-repeat left center;
	border: 1px dotted #00f;
	background-size: 16px;
	padding-left: 18px;
	cursor: auto;
}

.content .cke_contents_ltr img.cke_anchor {
	background:
		url(http://localhost:8080/Board/js/ckeditor/plugins/link/images/anchor.png?t=H8DA)
		no-repeat left center;
	border: 1px dotted #00f;
	background-size: 16px;
	width: 16px;
	min-height: 15px;
	height: 1.15em;
	vertical-align: text-bottom;
}

.content .cke_contents_rtl a.cke_anchor, 
.content .cke_contents_rtl a.cke_anchor_empty,
.content .cke_editable.cke_contents_rtl a[name], 
