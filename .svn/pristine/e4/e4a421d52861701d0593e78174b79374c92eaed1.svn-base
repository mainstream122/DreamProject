@charset "UTF-8";

#subtitle-area {
	position: relative;
	padding-top: 40px;
	width: 930px;
	height: 43px;
	border-bottom: 1px solid #eaeaea;
	margin: 0 auto;
}
#subtitle-area > img {
}
.location {
	position: absolute;
    top: 50px;
    right: 0;
    font-size: 11px;
    line-height: 1.2;
}
.location em {
    margin-left: 2px;
    padding-left: 7px;
    
    color: #55769f;
}
.location a {
    margin-left: 2px;
    padding-left: 7px;
    color: #747474;
}
#article {
	position: relative;
	width: 930px;
	margin: 0 auto;
	padding-top: 40px;
}
.title-area {
	width: 400px;
	height: 126px;
}
.title-area img {
	float: left;
	margin-bottom: 20px;
}
.title-area img:last-child {
	margin-bottom: 0px;
}

.title-area2 {
	position: absolute;
	top: 40px;
	left: 400px;
	width: 530px;
	height: 126px;
}
.title-area2 ol {
	list-style: none;
	padding-left:18px;
	
}
.title-area2 li {
	padding:5px 5px 5px 26px;
}
.title-area2 li:nth-child(1) {
	background: url("/DreamProject/img/global_menu/icon_num01.gif") no-repeat 0px 7px;
}
.title-area2 li:nth-child(2) {
	background: url("/DreamProject/img/global_menu/icon_num02.gif") no-repeat 0px 7px;
}
.title-area2 li:nth-child(3) {
	background: url("/DreamProject/img/global_menu/icon_num03.gif") no-repeat 0px 7px;
}
.title-area2 li:nth-child(4) {
	background: url("/DreamProject/img/global_menu/icon_num04.gif") no-repeat 0px 7px;
}
.title-area2 li:nth-child(5) {
	background: url("/DreamProject/img/global_menu/icon_num05.gif") no-repeat 0px 7px;
}
.customer_inquiry_information{
	width: 100%;
	margin-top: 50px;
    border-top: 2px solid #565d6a;
    border-bottom: 1px solid #e4e4e4;
}
.customer_inquiry_information img {
	border-right: 1px solid #eaeaea;
	float: left;
	
}
.customer_inquiry_information img:first-child {
	border-left: 0px;
}
.title-area {
	background:url("/DreamProject/img/global_menu/bg_title_area.gif") no-repeat 385px 0px;
}
.title-area2 li a {
	text-decoration: none;
	color:#555555;
}
.title-area2 li a:hover {
	text-decoration: underline;
	color:#555555;
}